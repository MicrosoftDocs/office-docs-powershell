{
  "build": {
    "content": [
      {
        "files": ["**/*.md"],
        "src": "docs-conceptual",
        "version": "teams-ps",
        "dest": "teams",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"]
      },
      {
         "files": ["toc.yml"],
        "src": "docs-conceptual",
        "version": "teams-ps",
        "dest": "teams/teams-ps"
      },
      {
        "files": ["**/*.yml"],
        "exclude": ["toc.yml"],
        "src": "teams-ps",
        "version": "teams-ps",
        "dest": "module"
      },
      {
        "files": ["toc.yml"],
        "src": "teams-ps",
        "version": "teams-ps",
        "dest": "module/teams-ps"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "virtual-folder",
        "version": "teams-ps",
        "dest": "module"
      }
